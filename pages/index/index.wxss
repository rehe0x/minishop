page,.page{
  width: 100%;
  height: 100%;
}

.head {
  flex-direction:column;
  background: #D9E1BF;
  height: 400rpx;
  width:100%;
  display: flex;
  position:absolute;
  left:0;
  top:0
}

.main{
  width: 100%;
  height: 100%;
}
.content {
  background: rgb(255, 255, 255);
  width:100%;
  border-radius: 40rpx 40rpx 0% 0%; 
  position:absolute;
  left:0rpx;
  top:350rpx;
  box-shadow: 0rpx 0rpx 10rpx -7rpx rgb(0, 0, 0);
  margin-bottom: 174rpx;
}
.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 70rpx 0rpx 0rpx 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  font-size: 60rpx;
  color: #3F3E3C;
  margin: 10rpx 0rpx 0rpx 20rpx;
}

.search{
  display: flex;
  padding-left: 108rpx;
}
.navigation{
  font-size: 30rpx;
  font-weight: 500;
  margin-left: 20rpx;
  margin-top: 24rpx;
}

.search-bar-box{
  width: 85%;
  height: 66rpx;
  display: flex;
  border-radius:40px;
  align-items: center;
  background: #F7F7F7;
  margin-top: 24rpx;
}

.search-bar-input{
  width: calc(100% - 122rpx);
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(152,153,159,1);
  line-height:36rpx;
  margin-right: 15rpx;
}
.icon-search{
  margin: 0 16rpx 0 20rpx;
}
.icon-microphone{
  width: 28rpx;
  height: 40rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}

.category{
  height: 90rpx;
  background: rgb(248, 248, 248);
  margin-top: 30rpx;
  border-top-style: solid;
  border-top-width: 1rpx;
  border-top-color: #f0eeee;
  border-bottom-style: solid;
  border-bottom-width: 1rpx;
  border-bottom-color: #f0eeee;
  display: flex;
  align-items: center;
}
.category-title{
  font-size: 35rpx;
  font-weight: 500;
  margin-left: 36rpx;
  color: #3F3E3C;
}
.category-title text{
  font-size: 30rpx;
  font-weight: 300;
  color: #9c9c9c;
}
.category-all{
  font-size: 30rpx;
  font-weight: 300;
  margin-left: 290rpx;
  color: #9c9c9c;
}

#category-tree{
  background: rgb(248, 248, 248);
  box-shadow: 0rpx 10rpx 22rpx -20rpx rgb(0, 0, 0);
  overflow: hidden;
  height: 0;
  width: 100%;
  animation-fill-mode: forwards;
  position: absolute;
  z-index: 999;
}

#top-nav{
  background: rgb(255, 255, 255);
  box-shadow: 0rpx 10rpx 22rpx -20rpx rgb(0, 0, 0);
  /* overflow: hidden; */
  height: 0;
  width: 100%;
  animation-fill-mode: forwards;
  position: absolute;
  z-index: 997;
}
@keyframes slidedown-top{
  from{
    height: 0;
  }
  to{
    height: 200rpx;
  }
}
@keyframes slideup-top{
  from{
    height: 200rpx;
  }
  to{
    height: 0rpx;
  }
}
.open-top{animation: slidedown-top 0.1s;} .close-top{animation: slideup-top 0.1s;} .hide-top{display: none;} .show-top{
display: block;}


#category-tree-top{
  background: rgb(248, 248, 248);
  box-shadow: 0rpx 10rpx 22rpx -20rpx rgb(0, 0, 0);
  overflow: hidden;
  height: 0;
  width: 100%;
  animation-fill-mode: forwards;
  position: absolute;
  z-index: 999;
}
@keyframes slidedown{
  from{
    height: 0;
  }
  to{
    height: 500rpx;
  }
}
@keyframes slideup{
  from{
    height: 500rpx;
  }
  to{
    height: 0rpx;
  }
}
.open{animation: slidedown 0.5s;} .close{animation: slideup 0.5s;} .hide{display: none;} .show{
display: block;}

.category-tree-name{
  font-size: 30rpx;
  font-weight: 300;
  margin-left: 36rpx;
  color: #9c9c9c;
  line-height: 107rpx;
}

.category-item{
  font-size: 30rpx;
  font-weight: 300;
  margin-left: 36rpx;
  margin-top: 1rpx;
  color: #9c9c9c;
}

.goodList-item{
  /* width: 100%; */
  height: 193rpx;
  display: flex;
  background: #ffffff;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:100;
  color:rgba(152,153,159,1);
  line-height:24rpx;
  margin: 20rpx 30rpx;
  box-sizing: border-box;
  /* border-bottom: 1rpx solid; */
  /* position: relative; */
}
.goods-img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 10%;
}

.item-text{
  margin-left: 25rpx;
  width: 505rpx;
  position: relative;
}
.item-name{
  font-size:40rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:#3F3E3C;
  line-height: 77rpx;
  /* margin-bottom: 4rpx; */
}
.item-sales{
  margin-bottom: 10rpx;
}

.item-specification{
  font-size:33rpx;
  font-family:PingFangSC-Semibold;
  font-weight:500;
  color:#3F3E3C;
  line-height: 50rpx;
}

.specification-select{
  width: 51px;
  height: 23px;
  background: #D9E1BF;
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  float: right;
  display: flex;
  align-items: center;
  justify-content: center;
}

.specification-button{
  font-size: 22rpx;
  color: #3F3E3C;
}


.float-button{
  position: fixed;
  width: 691rpx;
  height: 100rpx;
  background: #3F3E3C;
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  bottom: 40rpx;
  left: 30rpx;
  /* opacity: 0.8; */
  box-shadow: 0rpx 0rpx 10rpx -5rpx #3F3E3C;
  display: flex;
  z-index: 999;
}

.myorder{
  width: 25%;
  height: 100%;
  padding-left: 12px;
  padding-top: 9rpx;
  border-right-style: solid;
  border-right-width: 1px;
  border-right-color: #F2F2EA;
}
.myorder view{
  font-size: 26rpx;
  color: #F2F2EA;
  margin-top: -15rpx;
  margin-left: 20rpx;
}
.myorder image{
  width: 50rpx;
  height: 52rpx;
  margin-left: 49rpx;
  src: url("data:image/png;base64,");
}

.amount{
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.amount .amount-text{
  font-size: 61rpx;
  color: #D9E1BF;
}

.amount .number{
  width: 23px;
  height: 23px;
  background: #E26E54;
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  margin-bottom: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.amount .number view{
  font-size: 31rpx;
  color: #F2F2EA;
}

.tally{
  width: 25%;
  padding-left: 12px;
  padding-top: 20rpx;
  font-size: 47rpx;
  background: #D9E1BF;
  color: #3F3E3C;
  border-radius: 0rpx 100rpx 100rpx 0rpx;
}
